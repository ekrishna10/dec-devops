pipeline {
   agent any

   stages {
      stage('First') {
         steps {
            echo 'Hello World'
         }
      }
      stage('second') {
         steps {
            echo 'ipconfig'
         }
      }
      stage('Third') {
         steps {
            echo 'getmac'
         }
      }
      stage('Forth') {
         steps {
            echo 'netstat'
         }
      }	  
   }
}
